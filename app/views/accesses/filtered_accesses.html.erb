<p style="color: green"><%= notice %></p>

<h1>Accesses</h1>

<div id="accesses">
  <table class="table table-striped">
    <thead>
    <tr>
      <th scope="col">Ip_Origen</th>
      <th scope="col">Fecha</th>
    </tr>
    </thead>
  <tbody>
    <% @filtered_accesses.each do |access| %>
    <tr>
        <td><%= access.ip %></td>
        <td><%= access.created_at %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
  
</div>

<button onclick="goBack()">Back to Accesses</button>

<script>
  function goBack() {
    window.history.back();
  }
</script>

